{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node" 
        },
        {
            "src": "./build",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/saves",
            "dest": "/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/build/$1"
        }
    ]

}